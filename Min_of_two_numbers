import java.io.*;
import java.util.*;
public class Main {
    public static void main(String args[]) {
        // your code here
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int m = sc.nextInt();

        if (n<m){
            System.out.println(n+" is the Smallest number");
        } 
        else if (m<n){
            System.out.println(m+" is the Smallest number");
        }
        else{
            System.out.println("Both numbers are equal");
        }
    }
}
