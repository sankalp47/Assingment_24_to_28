import java.util.*;
import java.io.*;

public class Main {
    public static void main(String args[]) {
        //your code here
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter number: ");
        int n = sc.nextInt();
        int m = 0;
        for (int i=1;i<=n;i++){
          m = m + i;
          System.out.println(m);
        } 
    }    
}
