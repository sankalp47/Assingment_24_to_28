import java.io.*;
import java.util.*;
public class Main {
    public static void main(String args[]) {
        // your code here
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter First Number: ");
        int n = sc.nextInt();
        System.out.println("Enter Second Number: ");
        int m = sc.nextInt();

        System.out.println("The Average of two numbers is: " + (n + m)/2);
    }
}
