import java.util.*;
import java.io.*;

public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter Number: ");
        int n = sc.nextInt();
        int rev = 0;
        while(n!=0){
            int R = n%10;
            rev = rev * 10 + R;
            n = n/10;
        }
        System.out.println(rev);
    }
}
